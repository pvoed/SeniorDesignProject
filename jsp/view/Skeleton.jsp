<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/functions" prefix="fn" %>



<html>
        <head>          
                <style type="text/css">
                
                        .error
                        {
                                color: red;
                        }
                        
                        .text
                        {
                                color: black;
                                
                        }
                        
                        body
                        {
                        		background-size: 100%;
                        }
                        
                        .header
                        {
                                width:90%;
                                height:10%;
                                clear:both;
                                text-align:center;
                                background-color:#FFFFFF;
                                border:3px solid black;
                                opacity:0.9;
                                filter:alpha(opacity=90); /* For IE8 and earlier */
                        }
                        
                        .leftSidebar
                        {
                                width:22.5%;
                                height:80%;
                                float:left;
                                text-align:center;
                                background-color:#228B22;
                                border:1px solid black;
                                opacity:0.9;
                                filter:alpha(opacity=90); /* For IE8 and earlier */
                        }
                        
                        .body
                        {
                                width:45%;
                                height:80%;
                                overflow:auto;
                                float:left;
                                text-align:center;
                                background-color:#228B22;
                                border:1px solid black;
                                opacity:0.9;
                                filter:alpha(opacity=90); /* For IE8 and earlier */
                        }
                        
                        .rightSidebar
                        {
                                width:22.5%;
                                height:80%;
                                float:left;
                                text-align:center;
                                background-color:#228B22;
                                border:1px solid black;
                                opacity:0.9;
                                filter:alpha(opacity=90); /* For IE8 and earlier */
                        }
                        
                        .footer
                        {
                                color:red;
                                width:90%;
                                height:5%;
                                clear:both;
                                text-align:center;
                                background-color:#000000;
                                border:3px solid black;
                                opacity:0.9;
                                filter:alpha(opacity=90); /* For IE8 and earlier */
                        }
                </style>                
        </head>
</html>

